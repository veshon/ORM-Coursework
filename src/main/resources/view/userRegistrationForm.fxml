      <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label layoutX="255.0" layoutY="14.0" text="User Registration" />
            <TextField fx:id="txtName" layoutX="118.0" layoutY="143.0" prefHeight="25.0" prefWidth="189.0" promptText="Enter your name" />
            <TextField fx:id="txtPassword" layoutX="358.0" layoutY="143.0" prefHeight="25.0" prefWidth="189.0" promptText="Enter your password" />
            <TextField fx:id="txtEmail" layoutX="358.0" layoutY="85.0" prefHeight="25.0" prefWidth="189.0" promptText="Enter your e-mail address" />
